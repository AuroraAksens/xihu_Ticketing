/* pages/subpage/logDetails/logDetails.wxss */
.log-box{
  width: 90%;
  margin: 30rpx auto;
  background-color: white;
  box-shadow:0px 3px 12px rgba(123,174,178,0.16);
}
.log-box>.top{
  width: 100%;
  border-bottom: 1rpx solid #cfcfcf;
}
.log-box>.top>.left{
  width: 30%;
  color: #01CDCD;
  font-size: 26rpx;
  font-weight: bold;
}
.log-box>.top>.left>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.log-box>.top>.right{
  color: #646464;
  font-size: 20rpx;
  font-weight: bold;
  line-height: 60rpx;
}
.log-box>.foot{
  color: #BEBEBE;
  font-size: 22rpx;
  text-align: center;
  line-height: 60rpx;
}

.Tips{
  width: 100%;
  color: gray;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
}