/*  */
.m-orderlist {
  /* width: 90%;
  margin-left: 5%;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 25rpx; */
  height: calc(100vh - 200px);
  overflow: scroll;
  padding-bottom: 100rpx;
}

.m-orderlist>.orderlistBox {
  width: 90%;
  margin-left: 5%;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 25rpx;
  margin-top: 30rpx;
}

.m-orderlist>.orderlistBox>.top {
  border-bottom: 1rpx solid #e2e2e2;
}

.m-orderlist>.orderlistBox>.top>.text {
  font-size: 28rpx;
  font-weight: 550;
  color: rgba(235, 74, 66, 1);
  padding: 42rpx 42rpx 22rpx 42rpx;
}

.m-orderlist>.orderlistBox>.top>.xian {
  width: 90%;
  margin-left: 5%;
  background-color: rgba(216, 216, 216, 0.5);
  height: 2px;
}

.m-orderlist>.orderlistBox>.mid {
  display: flex;
  padding: 2%;
  width: 97%;
}

.m-orderlist>.orderlistBox>.mid>.left {
  width: 90%;
  margin-left: 5%;
  padding-bottom: 20rpx;
}

.m-orderlist>.orderlistBox>.mid>.right {
  display: flex;
  align-items: center;
}

.m-orderlist>.orderlistBox>.mid>.left>.mid1 {
  margin-top: 12rpx;
  font-size: 28rpx;
  font-weight: bold;
  color:  #992a31;
}

.m-orderlist>.orderlistBox>.mid>.left>.mid2 {
  margin-top: 12rpx;
  font-size: 23rpx;
  color: rgba(145, 131, 131, 1);
}

.m-orderlist>.orderlistBox>.mid>.left>.mid2>image {
  width: 20rpx;
  height: 20rpx;
}

.m-orderlist>.orderlistBox>.mid>.left>.mid3 {
  margin-top: 12rpx;
  font-size: 23rpx;
  color: rgba(145, 131, 131, 1);
}

.m-orderlist>.orderlistBox>.mid>.left>.mid3>image {
  width: 20rpx;
  height: 20rpx;
}

.m-orderlist>.orderlistBox>.mid>.left>.mid4 {
  margin-top: 15rpx;
  font-size: 23rpx;
  color: rgba(145, 131, 131, 1);
}

.m-orderlist>.orderlistBox>.mid>.left>.mid5 {
  margin-top: 25rpx;
  font-size: 23rpx;
  color: rgba(145, 131, 131, 1);
}

.m-orderlist>.orderlistBox>.mid>.right>image {
  width: 179rpx;
  height: 166rpx;
  border-radius: 10rpx;
}

.m-orderlist>.orderlistBox>.bottom {
  width: 90%;
  margin-left: 5%;
  padding-bottom: 30rpx;
}

.m-orderlist>.orderlistBox>.bottom>.xian {
  height: 2px;
  width: 100%;
}

.m-orderlist>.orderlistBox>.bottom>view>.btn {
  width: 170rpx;
  height: 60rpx;
  background: #01CDCD;
  border-radius: 10rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 65rpx;
  margin-top: 22rpx;
  margin-left: 15rpx;
}

.m-orderlist>.orderlistBox>.bottom>view>.btn1 {
  width: 170rpx;
  height: 60rpx;
  background: #BEBEBE;
  border-radius: 10rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 65rpx;
  margin-top: 22rpx;
  margin-left: 15rpx;
}


.nav-box {
  width: 135rpx;
  height: 55rpx;
  margin-right: 15rpx;
  margin-top: 20rpx;
  position: relative;
}

.nav-box>image {
  width: 100%;
  height: 100%;
}

.nav-box>.nav-text {
  width: 125rpx;
  height: 45rpx;
  background-color: white;
  text-align: center;
  line-height: 50rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  margin: auto;
  z-index: 5;
  border-radius: 10rpx;
  font-size: 22rpx;
  color: rgb(214 66 38);
}


.nav-box>.nav-text1 {
  width: 135rpx;
  height: 45rpx;
  background-color: white;
  text-align: center;
  line-height: 50rpx;
  position: absolute;
  top: 6rpx;
  right: 0;
  left: 0;
  margin: auto;
  z-index: 5;
  border-radius: 10rpx;
  font-size: 22rpx;
  color: black;
  font-weight: bold;
}

.m-orderlist>.orderlistBox>.bottom>view {
  display: flex;
}

